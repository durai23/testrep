            <para PARENT="Command"/>
            <ITEXT CH="Setting up the environment"/>
            <para PARENT="Heading"/>
            <ITEXT CH="Several compilers are available. To get a list use the module command"/>
            <para/>
            <ITEXT CH="module avail "/>
            <para PARENT="Command"/>
            <ITEXT CH="To load CUDA first load a toolchain, e.g, "/>
            <para/>
            <ITEXT CH="module load intel-para"/>
            <para PARENT="Command"/>
            <ITEXT CH="and then the module"/>
            <para/>
            <ITEXT CH="module load CUDA/7.5.18"/>
            <para PARENT="Command"/>
            <ITEXT CH="Accessing a compute node"/>
            <para PARENT="Heading"/>
            <ITEXT CH="The frontend nodes can be used for development but to run your code on a GPU you need to access one of the compute nodes. To get a node with four GPUs for 8 hours use"/>
            <para/>
            <ITEXT CH="salloc --reservation=cuda -p gpus  --nodes=1 --gres=gpu:4 --time=8:0:0"/>
            <para PARENT="Command"/>
            <ITEXT CH="After some time you are returned to the prompt. To run a program on the compute node start it with "/>
            <para/>
            <ITEXT CH="srun &lt;executable&gt;"/>
            <para PARENT="Command"/>
            <ITEXT CH="To start an interactive session use"/>
            <para/>
            <ITEXT CH="srun --forward-x --cpu_bind=none --pty /bin/bash -i"/>
            <trail PARENT="Command"/>
            <PageItemAttributes/>
        </PAGEOBJECT>
